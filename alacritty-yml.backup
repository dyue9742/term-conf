window:
  padding:
    x: 0
    y: 0
  dynamic_padding: true
  decorations: none
  startup_mode: SimpleFullscreen

font:
  normal:
    family: 'JetBrainsMono Nerd Font'
    style: Regular
  bold:
    family: 'JetBrainsMono Nerd Font'
    style: Bold
  italic:
    family: 'JetBrainsMono Nerd Font'
    style: Italic
  bold_italic:
    family: 'JetBrainsMono Nerd Font'
    style: Bold Italic
  size: 23.0

shell:
  program: /usr/local/bin/tmux

alt_send_esc: false
